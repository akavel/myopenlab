import java.util.ArrayList;

/**
 *
 * @author Carmelo
 */
public class Properties
{
    private ArrayList list = new ArrayList();

    public Property get(int index)
    {
        return (Property)list.get(index);
    }

    public void add(Property property)
    {
        list.add(property);
    }

    public int size()
    {
        return list.size();
    }


}
